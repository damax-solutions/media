{
  "name": "damax/media",
  "type": "library",
  "description": "Media management.",
  "license": "MIT",
  "autoload": {
    "psr-4": { "Damax\\Media\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Damax\\Media\\Tests\\": "tests/" }
  },
  "require": {
    "php": ">=7.1",
    "beberlei/assert": "^2.9",
    "damax/common": "^1.0",
    "ramsey/uuid": "^3.7",
    "symfony/http-foundation": "^4.0"
  },
  "require-dev": {
    "doctrine/orm": "^2.6",
    "friendsofphp/php-cs-fixer": "^2.10",
    "knplabs/gaufrette": "^0.6.0",
    "league/flysystem": "^1.0",
    "league/glide": "^1.3",
    "league/glide-symfony": "^1.0",
    "matthiasnoback/symfony-dependency-injection-test": "^2.3",
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/doctrine-bridge": "^4.0",
    "symfony/form": "^4.0",
    "symfony/phpunit-bridge": "^4.0",
    "symfony/serializer": "^4.0",
    "symfony/validator": "^4.0",
    "twig/twig": "^2.4"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "repositories": [
    { "type": "vcs", "url": "git@github.com:lakiboy/damax-common.git" }
  ]
}
